<page xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" layout="1column" xsi:noNamespaceSchemaLocation="urn:framework:View/Layout/etc/page_configuration.xsd">
  <body>
    <referenceContainer name="header.container">
      <block class="Magento\Cms\Block\Block" name="aboutHero">
        <arguments>
          <argument name="block_id" xsi:type="string">quote_history_hero</argument>
        </arguments>
      </block>
    </referenceContainer>
    <referenceBlock name="page.main.title">
      <action method="setPageTitle">
        <argument translate="true" name="title" xsi:type="string">Quote History</argument>
      </action>
    </referenceBlock>
    <referenceContainer name="content">
      <block class="RTech\Quote\Block\Quote\History" name="quote.history" template="RTech_Quote::quotes/history.phtml" cacheable="false"/>
      <block class="Magento\Customer\Block\Account\Dashboard" name="customer.account.link.back" template="Magento_Customer::account/link/back.phtml" cacheable="false"/>
    </referenceContainer>
  </body>
</page>
